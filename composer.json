{
    "name": "axy/errors",
    "type": "library",
    "description": "Exceptions in PHP",
    "keywords": [
        "Exception",
        "Error"
    ],
    "homepage": "https://github.com/axypro/errors",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Grigoriev",
            "email": "go.vasac@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "axy/backtrace": "~2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "axy\\errors\\": "src",
            "axy\\errors\\tests\\": "tests"
        }
    },
    "archive": {
        "exclude": ["/tests/*", "/doc/*", "/cs.sh", "/phpmd.sh", "/*.xml.dist", "/CHANGELOG.md"]
    }
}
